        android:id="@+id/IntoJSONobjectmethod"
        android:text="@string/IntoJSONobjectmethod"
        android:textAllCaps="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/IntoGSONmethod"
        android:text="@string/IntoGSONmethod"
        android:textAllCaps="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/Testchat"
        android:text="@string/Testchat"
        android:textAllCaps="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>